reset=\"onReset\">\r\n      <div>\r\n        <label>name</label>\r\n        <input v-model=\"name\" />\r\n      </div>\r\n\r\n      <div>\r\n        <label>email</label>\r\n        <input v-model=\"email\" />\r\n      </div>\r\n\r\n      <div>\r\n        <label>message</label>\r\n        <textarea v-model=\"message\"></textarea>\r\n      </div>\r\n\r\n      <button type=\"submit\">submit</button>\r\n      <button type=\"reset\">reset</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'ContactMe',\r\n  data () {\r\n    return {\r\n      name: '',\r\n      email: '',\r\n      message: ''\r\n    }\r\n  },\r\n  computed: {\r\n    formValid () {\r\n      const { name, email, message } = this\r\n      return (\r\n        name.length > 0 &&\r\n        /(.+)@(.+){2,}.(.+){2,}/.test(email) &&\r\n        message.length > 0\r\n      )\r\n    }\r\n  },\r\n  methods: {\r\n    onReset () {\r\n      this.name = ''\r\n      this.email = ''\r\n      this.message = ''\r\n    },\r\n    submit () {\r\n      if (!this.formValid) {\r\n        return\r\n      }\r\n      if (!localStorage.getItem('messages')) {\r\n        localStorage.setItem('messages', JSON.stringify([]))\r\n      }\r\n      const messages = JSON.parse(localStorage.getItem('messages'))\r\n      const { name, email, message } = this\r\n      messages.push({\r\n        name,\r\n        email,\r\n        message\r\n      })\r\n      localStorage.setItem('messages', JSON.stringify(messages))\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}